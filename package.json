{
  "name": "express",
  "version": "1.0.0",
  "description": "",
  "main": "server.ts",
  "_moduleAliases": {
    "@": "build/src",
    "@core": "build/src/core",
    "@entity": "build/src/entity",
    "@utils": "build/src/utils"
  },
  "scripts": {
    "build": "npx tsc && npm run copy-files",
    "start": "node build/server.js",
    "dev": "concurrently \"npx tsc --watch\" \"npm run copy-files\" \"nodemon -q build/server.js\"",
    "copy-files": "copyfiles -u 1 **/*.graphql build/ && copyfiles -u 1 src/views/**/*.* build/src && copyfiles -u 1 src/public/**/*.* build/src"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.0",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "ffmpeg-extract-frames": "^2.0.2",
    "firebase-admin": "^11.9.0",
    "jsonwebtoken": "^9.0.0",
    "module-alias": "^2.2.2",
    "mongoose": "^7.1.1",
    "mongoose-sequence": "^5.3.1",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.3",
    "sharp": "^0.32.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^20.1.5",
    "concurrently": "^8.0.1",
    "copyfiles": "^2.4.1",
    "nodemon": "^2.0.22",
    "typescript": "^5.0.4"
  }
}
